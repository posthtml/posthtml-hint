{
  "name": "posthtml-hint",
  "version": "0.8.3",
  "description": "HTML Hint for PostHTML",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "main": "index.js",
  "scripts": {
    "pretest": "standard",
    "test": "nyc ava --verbose",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "nyc": {
    "all": true,
    "require": "babel-register",
    "include": [
      "lib",
      "test"
    ],
    "extension": [
      ".js"
    ]
  },
  "standard": {
    "ignore": []
  },
  "dependencies": {
    "htmlhint": "0.11.0",
    "log-symbols": "3.0.0",
    "posthtml-render": "1.1.4",
    "text-table": "0.2.0"
  },
  "devDependencies": {
    "ava": "5.2.0",
    "babel-register": "^6.26.0",
    "coveralls": "3.0.9",
    "nyc": "15.0.0",
    "posthtml": "0.12.0",
    "standard": "14.3.1"
  },
  "keywords": [
    "HTML",
    "HTMLHINT",
    "PostHTML",
    "PostHTML Hint"
  ],
  "author": {
    "name": "Michael Ciniawky",
    "email": "michael.ciniawsky@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/posthtml/posthtml-hint"
  },
  "bugs": {
    "url": "https://github.com/posthtml/posthtml-hint/issues"
  },
  "homepage": "https://github.com/posthtml/posthtml-hint#readme",
  "license": "MIT"
}
